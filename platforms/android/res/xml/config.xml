<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mm.movimetro" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <splash density="land-hdpi" src="www/img/Bienvenida-01.png" />
    <splash density="land-ldpi" src="www/img/Bienvenida-01.png" />
    <splash density="land-mdpi" src="www/img/Bienvenida-01.png" />
    <splash density="land-xhdpi" src="www/img/Bienvenida-01.png" />
    <splash density="port-hdpi" src="www/img/Bienvenida-01.png" />
    <splash density="port-ldpi" src="www/img/Bienvenida-01.png" />
    <splash density="port-mdpi" src="www/img/Bienvenida-01.png" />
    <splash density="port-xhdpi" src="www/img/Bienvenida-01.png" />
    <name>MoviMetro</name>
    <description>
        MoviMetro
    </description>
    <author email="movimetro@gmail.com" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <icon density="mdpi" height="57" src="res/icon.png" width="57" />
    <preference name="SplashScreenDelay" value="10000" />
</widget>
